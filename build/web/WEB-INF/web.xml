       <servlet>
    <servlet-name>camp_ad</servlet-name>
    <servlet-class>camp_add</servlet-class>
    <multipart-config>
        <!-- Configuration for multipart handling -->
        <max-file-size>5242880</max-file-size> <!-- Maximum size allowed for uploaded files (5 MB in this example) -->
        <max-request-size>10485760</max-request-size> <!-- Maximum size allowed for multipart/form-data requests (10 MB in this example) -->
        <file-size-threshold>0</file-size-threshold> <!-- Size threshold after which files will be written to disk (0 means always) -->
    </multipart-config>
</servlet>
<servlet-mapping>
    <servlet-name>camp_ad</servlet-name>
    <url-pattern>/camp_ad</url-pattern>
</servlet-mapping>


